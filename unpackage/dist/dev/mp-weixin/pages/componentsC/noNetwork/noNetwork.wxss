@import "../../../nvue.wxss";
/**
 * 下方引入的为uView UI的集成样式文件，为scss预处理器，其中包含了一些"u-"开头的自定义变量
 * 使用的时候，请将下面的一行复制到您的uniapp项目根目录的uni.scss中即可
 * uView自定义的css类名和scss变量，均以"u-"开头，不会造成冲突，请放心使用 
 */
.u-content {
  padding: 150px 60px 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.u-content__circle {
  background-color: #5ac725;
  display: flex;
  flex-direction: row;
  border-radius: 100px;
  width: 60px;
  height: 60px;
  align-items: center;
  justify-content: center;
}
.u-content__normal {
  font-size: 15px;
  color: #5ac725;
  margin-top: 15px;
}
.u-content__tips {
  color: #909193;
  font-size: 13px;
  margin-top: 15px;
  text-align: center;
}